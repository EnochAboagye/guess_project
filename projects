import random

def get_level_range(level):
    """
    Determines the maximum number for the current level's range.
    The minimum number is always 1.
    """
    # Define the pattern for the max number based on the level
    if level == 1:
        max_num = 5
    elif level == 2:
        max_num = 10
    elif level == 3:
        max_num = 20
    elif level == 4:
        max_num = 25
    elif level == 5:
        max_num = 35
    elif level == 6:
        max_num = 50
    elif level == 7:
        max_num = 65
    elif level == 8:
        max_num = 75
    elif level == 9:
        max_num = 90
    elif level == 10:
        max_num = 100
 
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
    